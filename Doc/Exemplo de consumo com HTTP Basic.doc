<div class=3D"code panel pdl" style=3D"border-width: 1px;">
<div class=3D"codeContent panelContent pdl">=20
<pre class=3D"syntaxhighlighter-pre" data-syntaxhighlighter-params=3D"brush=
: xml; gutter: true; theme: Confluence" data-theme=3D"Confluence">#include =
"totvs.ch"

User Function restSample()
Local oRestClient :=3D FWRest():New("http://localhost:8080")
Local aHeader :=3D {"tenantId: 99,01"}

// inclui o campo Authorization no formato &lt;usuario&gt;:&lt;senha&gt; na=
 base64
Aadd(aHeader, "Authorization: Basic " + Encode64("admin:123456"))

// chamada da classe exemplo de REST com retorno de lista=20
oRestClient:setPath("/rest/sample?startIndex=3D2&amp;count=3D10")
If oRestClient:Get(aHeader)
&nbsp;&nbsp; ConOut("GET", oRestClient:GetResult())
Else
&nbsp;&nbsp; ConOut("GET", oRestClient:GetLastError())
EndIf
&nbsp;
// chamada da classe exemplo de REST para opera=C3=A7=C3=B5es CRUD
oRestClient:setPath("/rest/sample/1")
If oRestClient:Get(aHeader)
&nbsp;&nbsp; ConOut("GET", oRestClient:GetResult())
Else
&nbsp;&nbsp; ConOut("GET", oRestClient:GetLastError())
EndIf

&nbsp;
// define o conte=C3=BAdo do body
oRestClient:SetPostParams("body")
&nbsp;
If oRestClient:Post(aHeader)
&nbsp;&nbsp; ConOut("POST", oRestClient:GetResult())
Else
&nbsp;&nbsp; ConOut("POST", oRestClient:GetLastError())
EndIf

&nbsp;
// para os m=C3=A9todos PUT e DELETE o conte=C3=BAdo body =C3=A9 enviado po=
r parametro
If oRestClient:Put(aHeader, "body")
&nbsp;&nbsp; ConOut("PUT", oRestClient:GetResult())
Else
&nbsp;&nbsp; ConOut("PUT", oRestClient:GetLastError())
EndIf

If oRestClient:Delete(aHeader, "body")
&nbsp;&nbsp; ConOut("DELETE", oRestClient:GetResult())
Else
&nbsp;&nbsp; ConOut("DELETE", oRestClient:GetLastError())
EndIf
Return&nbsp;</pre>=20
